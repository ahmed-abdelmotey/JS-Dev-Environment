{
  "name": "JS-Dev-Environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "scripts": {
    "test-security": "nsp check",
    "prestart": "babel-node _buildScripts/startMessage.js",
    "start": "npm-run-all --parallel test-security start:src lint:watch test:watch",
    "start:src": "babel-node _buildScripts/srcServer.js",
    "local-tunnel": "lt --port 3000 --subdomain mo3ty",
    "share": "npm-run-all --parallel start:src local-tunnel",
    "lint": "esw webpack.config.* _buildScripts src --color",
		"lint:watch": "npm run lint -- --watch",
		"test": "mocha --reporter progress _buildScripts/testSetup.js \"src/**/*.spec.js\"",
		"test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmed-abdelmotey/JS-Dev-Environment.git"
  },
  "keywords": [],
  "author": "Ahmed Abdelmotey",
  "license": "MIT",
  "homepage": "https://github.com/ahmed-abdelmotey/JS-Dev-Environment#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.24.5",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "connect-browser-sync": "^2.1.0",
    "cors": "^2.8.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-watch": "^4.0.1",
    "express": "^4.16.3",
    "jsdom": "^11.11.0",
    "localtunnel": "^1.9.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "nsp": "^3.2.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-md5-hash": "0.0.6"
  }
}
